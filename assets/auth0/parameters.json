{
  "PROJECT_LOOKUP": {
    "Order": 1,
    "Prompts": [
      {
        "Type": "eac:projects|select",
        "Message": "Choose the project to deploy the application to?",
        "Arguments": {
          "CreateText": "Create a project"
        }
      }
    ]
  },
  "ORGANIZATION": {
    "Order": 2,
    "Prompts": [
      {
        "Type": "git:organizations|select",
        "Message": "Enter the Organization name:"
      }
    ]
  },
  "REPOSITORY": {
    "Order": 3,
    "Prompts": [
      {
        "Message": "Enter the name of the new repository:"
      }
    ]
  },
  "AUTH0_DOMAIN": {
    "Order": 4,
    "Prompts": [
      {
        "Message": "Enter your Auth0 domain:"
      }
    ]
  },
  "AUTH0_CLIENT_ID": {
    "Order": 5,
    "Prompts": [
      {
        "Message": "Enter your Auth0 Client ID:"
      }
    ]
  },
  "PACKAGE_ROUTE": {
    "Order": 6,
    "Prompts": [
      {
        "Type": "utils:route",
        "Message": "Enter the route to run the application on:"
      }
    ]
  }
